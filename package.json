{
  "name": "shp-kit",
  "version": "0.4.1",
  "description": "Read & write shapefiles on the modern web.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": false,
  "homepage": "https://github.com/abrman/shp-kit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/abrman/shp-kit"
  },
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "ci": "npm run lint && npm run test && npm run build",
    "release": "npm run lint && npm run test && npm run build && changeset publish"
  },
  "keywords": [
    "shapefile",
    "geojson",
    "shp"
  ],
  "author": "Matthew Abrman",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/geojson": "^7946.0.13",
    "@types/proj4": "^2.5.5",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@turf/helpers": "^7.1.0",
    "@turf/turf": "^7.1.0",
    "buffer": "^6.0.3",
    "jszip": "^3.10.1",
    "proj4": "^2.10.0"
  }
}
